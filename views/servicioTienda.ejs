<%- include ('template/cabecera', {tituloweb: 'Servicio-Tienda'}); %>
<div class="mx-5 my-5">
<h1>Servicio-Tienda</h1>
<form action="/servicioTienda" class="my-2" method="post">
    <select name="id_tienda" id="" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
    <% if(arrayTiendas.length>0){ %>
        <% arrayTiendas.forEach(tienda=>{ %>
            <option value="<%= tienda.id %>"><%= tienda.nombre %></option>
        <%})%>
    <%}%>
    </select>
    <select name="id_servicio" id="" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
    <% if(arrayServicios.length>0){ %>
        <% arrayServicios.forEach(servicio=>{ %>
            <option value="<%= servicio.id %>"><%= servicio.descripcion %></option>
        <%})%>
    <%}%>
    </select>
    <button type="submit" class="btn btn-dark btn-block">Agregar</button>
</form>
</div>
<%- include('template/footer'); %>