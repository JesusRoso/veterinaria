<%- include ('template/cabecera', {tituloweb: 'Propietario-Mascota'}); %>
<div class="mx-5 my-5">
<h1>Propietario-Mascota</h1>
<form action="/propietarioMascota" class="my-2" method="post">
    <select name="id_propietario" id="" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
    <% if(arrayPropietarios.length>0){ %>
        <% arrayPropietarios.forEach(propietario=>{ %>
            <option value="<%= propietario.id %>"><%= propietario.nombre %></option>
        <%})%>
    <%}%>
    </select>
    <select name="id_mascota" id="" class="form-select form-select-lg mb-3" aria-label=".form-select-lg example">
    <% if(arrayMascotas.length>0){ %>
        <% arrayMascotas.forEach(mascota=>{ %>
            <option value="<%= mascota.id %>"><%= mascota.nombre %></option>
        <%})%>
    <%}%>
    </select>
    <button type="submit" class="btn btn-dark btn-block">Agregar</button>
</form>
</div>
<%- include('template/footer'); %>