<%- include ('template/cabecera', {tituloweb: 'Crear Propietario'}); %>
<div class="container">
    <h1>Crear Propietario</h1>
    <form id="crearPropietario" action="/propietarios" class="my-2" method="post">
        <input type="text" placeholder="Ingrese nombre" class="form-control my-2" name="nombre">
        <input type="text" placeholder="Ingrese apellido" class="form-control my-2" name="apellido">
        <input type="text" placeholder="Ingrese documento" class="form-control my-2" name="documento">
        <input type="text" placeholder="Ingrese la edad" class="form-control my-2" name="edad">
        <input type="text" placeholder="Ingrese el contacto" class="form-control my-2" name="contacto">
        <button type="submit" class="btn btn-dark btn-block">Agregar</button>
    </form>
</div>
<script>
    const formCrearPropietario = document.querySelector("#crearPropietario");
    formCrearPropietario.addEventListener("submit",()=>{
        alert("Creado de forma correcta");
    })
</script>
